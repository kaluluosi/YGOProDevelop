<UserControl x:Class="YGOProDevelop.View.CardListView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:YGOProDevelop.View"
             mc:Ignorable="d" 
             d:DesignHeight="312.232" d:DesignWidth="122.962" >
    <Grid >
        <Grid.RowDefinitions>
            <RowDefinition Height="145*"/>
            <RowDefinition Height="11*"/>
        </Grid.RowDefinitions>
        <ListBox x:Name="listBox" ItemsSource="{Binding CdbService.QueryResult}" DisplayMemberPath="texts.name"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="22" Grid.Row="1" TextWrapping="Wrap" Text="Search" VerticalAlignment="Top" Width="123"/>
    </Grid>
</UserControl>
